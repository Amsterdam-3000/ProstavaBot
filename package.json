{
  "name": "prostavabot",
  "version": "0.2.0",
  "description": "Я контролирую проставы твоих друзей",
  "scripts": {
    "start-dev": "nodemon src/bot.ts",
    "serve-dev": "nodemon src/server.ts",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "build-ts": "tsc",
    "build": "npm run lint && npm run build-ts && npm run copy-static-assets",
    "start": "node dist/bot.js",
    "serve": "node dist/server.js"
  },
  "nodemonConfig": {
    "verbose": true,
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts,json,yaml,ejs",
    "delay": 3000
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usebooz/ProstavaBot.git"
  },
  "author": "Dmitry Sternyaev <dimastv@yandex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/usebooz/ProstavaBot/issues"
  },
  "homepage": "https://github.com/usebooz/ProstavaBot#readme",
  "dependencies": {
    "@edjopato/telegraf-i18n": "^0.2.2",
    "@touch4it/ical-timezones": "^1.7.0",
    "body-parser": "^1.19.0",
    "bull": "^3.22.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "emoji-regex": "^9.2.2",
    "emoji-toolkit": "^6.5.1",
    "emoji-unicode": "^2.0.1",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "ical-generator": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.27.0",
    "mongoose": "^5.12.4",
    "mongoose-autopopulate": "^0.13.0",
    "node-fetch": "^2.6.1",
    "telegraf": "^4.3.0",
    "telegraf-calendar-telegram": "^1.5.8",
    "telegraf-session-redis": "^5.1.0",
    "zodiac-signs": "^1.1.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/bull": "^3.15.1",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.12",
    "@types/express-jwt": "^6.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/luxon": "^1.26.5",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
