{
  "name": "prostavabot",
  "version": "0.2.0",
  "description": "Я контролирую проставы твоих друзей",
  "scripts": {
    "start-dev": "nodemon src",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "build-ts": "tsc",
    "build": "npm run lint && npm run build-ts && npm run copy-static-assets",
    "start": "node dist"
  },
  "nodemonConfig": {
    "verbose": true,
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts,json,yaml,ejs",
    "delay": 3000,
    "exec": "ts-node --files=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usebooz/ProstavaBot.git"
  },
  "author": "Dmitry Sternyaev <dimastv@yandex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/usebooz/ProstavaBot/issues"
  },
  "homepage": "https://github.com/usebooz/ProstavaBot#readme",
  "dependencies": {
    "@edjopato/telegraf-i18n": "^0.2.2",
    "@touch4it/ical-timezones": "^1.8.1",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "bull": "^3.29.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "emoji-regex": "^10.0.0",
    "emoji-toolkit": "^6.6.0",
    "emoji-unicode": "^2.0.1",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "ical-generator": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.0.2",
    "mongoose": "^6.0.9",
    "mongoose-autopopulate": "^0.16.0",
    "morgan": "^1.10.0",
    "telegraf": "^4.4.2",
    "telegraf-calendar-telegram": "^1.5.8",
    "telegraf-session-redis": "^5.1.0",
    "zodiac-signs": "^1.1.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/bull": "^3.15.5",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/luxon": "^2.0.4",
    "@types/mongoose-autopopulate": "^0.10.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.3",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "shelljs": "^0.8.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
