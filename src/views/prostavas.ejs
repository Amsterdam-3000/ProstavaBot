<% if (date) { %><b><%= DateUtils.getDateString(i18n.languageCode, date) %></b><% } %>
<% if (!prostavas?.length) { %>
<%= i18n.t('reply.prostava.no_prostavas', { code: CODE.COMMAND.REQUEST }) %>
<% } else { %><% prostavas.forEach(function(prostava){ %>
<%= prostava.title %><% if (prostava.status) { %> • <%= LocaleUtils.getStatusText(i18n, prostava.status) %><% } %>
<%= prostava.author.user_string %><% if (isReminders) { %> • <%= DateUtils.getDateString(i18n.languageCode, prostava.closing_date) %><% } %>
<% }); %>
<% } %>